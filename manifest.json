{
    "name": "Highlight some phrases",
    "description": "Hightlight some pre defined text after page loads",
    "version": "0.1",
    "permissions": [
        "tabs","<all_urls>"
    ],
    "browser_action": {
        "default_icon": "images/get_started128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
                ],
            "css": ["content.css"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ], 
    "manifest_version": 2
}